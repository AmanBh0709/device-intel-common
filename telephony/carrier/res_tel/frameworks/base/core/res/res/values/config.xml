<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- When true, setting of preferred rat is allowed -->
    <add-resource type="bool" name="config_set_preferred_rat_allowed" />
    <bool name="config_set_preferred_rat_allowed">true</bool>

    <!-- When true, operator name display is customzied -->
    <add-resource type="bool" name="config_customize_operator_name_display" />
    <bool name="config_customize_operator_name_display">false</bool>

    <!-- When true, 5 level signal bar is shown in status bar -->
    <add-resource type="bool" name="config_signalStrengthExt" />
    <bool name="config_signalStrengthExt">false</bool>

    <add-resource type="array" name="plmn" />
    <string-array name="plmn">
        <item>CHN-MOBILE</item>
        <item>CHN-UNICOM</item>
        <item>CHN-CTNET</item>
    </string-array>

    <add-resource type="bool" name="config_set_correct_carrier_name" />
    <bool name="config_set_correct_carrier_name">false</bool>

    <!-- When true, publish enhancedtelepohny service /-->
    <add-resource type="bool" name="config_enhancedtelephony_service_enabled" />
    <bool name="config_enhancedtelephony_service_enabled">false</bool>

    <add-resource type="array" name="plmnIndex" />
    <integer-array name="plmnIndex">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </integer-array>
    <add-resource type="bool" name="config_capability_show_chinese" />
    <bool name="config_capability_show_chinese">false</bool>

    <!--  When true, mms can backup the received message -->
    <add-resource type="bool" name="config_backup_received_message" />
    <bool name="config_backup_received_message">false</bool>

    <!--  When true, enable sending and receing MMS on both SIM Card -->
    <add-resource type="bool" name="config_mms_dual_sim_feature" />
    <bool name="config_mms_dual_sim_feature">false</bool>

    <!-- When true, enable sms validity period -->
    <add-resource type="bool" name="config_sms_validity_period_allowed" />
    <bool name="config_sms_validity_period_allowed">false</bool>
</resources>
